                fd.append("vergidairesi", $("#vergidairesi").val());
                fd.append("vergino", $("#vergino").val());

                

                $.ajax({
                    data: fd,
                    type: "POST",
                    url: "AboneOl.ashx",
                    cache: false,
                    contentType: false,
                    processData: false,
                    success: function (url) {
                         $("#basvuruno").html(url);
                        
                    }
                })

                     
            }
                
           
               
             )


            function bekleyin() {

                setTimeout(function(){

                    $("#beklersin").hide();
                    $("#bankabilgileri").slideDown();

                    

                },5000)

            }

            $("#adresturet").click(function () {


                $("#fadres").val($("#teslimatadres").val());
                $("#tpk").val($("#fpk").val());

                $("#fadres").focus();


                var il = $("#illist").find("option:selected").attr('value');
                var ilce = $("#ilcelist").find("option:selected").attr('value');

                $("#illist1 > option[value="+il+"]").attr("selected", true);

                $('#ilcelist1').html($('<option></option>').html("Merkez").val("Merkez"));

                $.each(konum[il], function (key, value) {

                    $('#ilcelist1').append($('<option></option>').html(value).val(value));

                })
 


                $("#ilcelist1 > option[value=" + ilce + "]").attr("selected", true);


            });
        
                for (var p in konum) {
                    if (konum.hasOwnProperty(p)) {
                        $('#illist').append($('<option></option>').html(p).val(p));
                        $('#illist1').append($('<option></option>').html(p).val(p));

                    }
                }

                function Adanaicin() {
                    $("#illist > option[value=Adana]").attr("selected", true);
                    $("#illist1 > option[value=Adana]").attr("selected", true);


                    $.each(konum["Adana"], function (key, value) {

                        $('#ilcelist').append($('<option></option>').html(value).val(value));
                        $('#ilcelist1').append($('<option></option>').html(value).val(value));

                    })
                    $("#ilcelist > option[value=Aladağ]").attr("selected", true);
                    $("#ilcelist1 > option[value=Aladağ]").attr("selected", true);

                }
            


                Adanaicin();




                $('#illist').change(function () {
                    var il = $(this).find("option:selected").attr('value');
                
                    $('#ilcelist').html($('<option></option>').html("Merkez").val("Merkez"));
                    $("#ilcelist > option[value=Merkez]").attr("selected", true);


                    $.each(konum[il],function(key,value){
                    
                     $('#ilcelist').append($('<option></option>').html(value).val(value));

                    }) 
                });
                $('#illist1').change(function () {
                    var il = $(this).find("option:selected").attr('value');

                    $('#ilcelist1').html($('<option></option>').html("Merkez").val("Merkez"));
                    $("#ilcelist1 > option[value=Merkez]").attr("selected", true);


                    $.each(konum[il], function (key, value) {

                        $('#ilcelist1').append($('<option></option>').html(value).val(value));

